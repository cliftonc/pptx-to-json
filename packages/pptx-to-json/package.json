{
  "name": "pptx-to-json",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "log-bin": "tsx src/scripts/log-paste-bin.ts",
    "regenerate-test-cases": "tsx src/scripts/regenerate-test-cases.ts",
    "download-fixtures": "node src/scripts/download-fixtures.js",
    "add-test-case": "node src/scripts/add-test-case.js",
    "validate-samples": "node src/scripts/validate-samples.js"
  },
  "dependencies": {
    "fast-xml-parser": "^5.2.5",
    "jszip": "^3.10.1"
  },
  "optionalDependencies": {
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^1.6.0",
    "fracturedjsonjs": "^2.2.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
